{
    "name": "sniccowp/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "calvinalkan",
            "email": "calvin@snicco.de"
        },
        {
            "name": "Calvin Alkan",
            "email": "calvin@snicco.de"
        }
    ],
    "type": "library",
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-mysqli": "*",
        "akrabat/ip-address-middleware": "^2.0",
        "illuminate/database": "^v8.0.0",
        "illuminate/view": "^v8.0.0",
        "nesbot/carbon": "^2.31",
        "nikic/fast-route": "1.3.0",
        "nyholm/psr7-server": "^1.0-stable",
        "psr/http-message": "^1.0.0",
        "psr/http-server-middleware": "1.0.0",
        "psr/log": "^1.0.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/finder": "^5.0-stable",
        "symfony/http-kernel": "^5.0",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "require-dev": {
        "adriansuter/php-autoload-override": "^1.2",
        "bacon/bacon-qr-code": "^2.0",
        "bjeavons/zxcvbn-php": "^1.0",
        "defuse/php-encryption": "^2.3",
        "fakerphp/faker": "^1.0.0-stable",
        "filp/whoops": "^2.2",
        "illuminate/container": "^8.0.0",
        "lucatume/wp-browser": "^3.0.0",
        "mockery/mockery": "^1.4.2",
        "nyholm/psr7": "^1.0.0-stable",
        "opis/closure": "3.6.2",
        "phpunit/phpunit": "^9.5",
        "pragmarx/google2fa": "8.0.0",
        "respect/validation": "2.2.3",
        "roots/wp-password-bcrypt": "dev-master",
        "symplify/monorepo-builder": "^9.4"
    },
    "suggest": {
        "nyholm/psr7": "A minimal and very fast PSR7 implementation",
        "opis/closure": "Required if you want to cache routes that use closures [3.6.2]",
        "illuminate/encryption": "Required if you want to use strong encryption [8.40.0]",
        "filp/whoops": "Required if you want the pretty page error handler during devolopment",
        "respect/validation": "Required if you want to use the validation module",
        "bjeavons/zxcvbn-php": "A package by Dropbox to enfore validate password entroypy. Required for the auth package.",
        "roots/wp-password-bcrypt": "Required for the auth package for better wordpress password hashing",
        "bacon/bacon-qr-code": "Required for the auth package to genereate two-factor qr codes",
        "pragmarx/google2fa": "Required for the auth package to for 2FA Authentication [8.0.0]"
    },
    "autoload": {
        "files": [
            "packages/better-wp-hooks/src/functions.php",
            "src/Support/functions.php"
        ],
        "psr-4": {
            "Snicco\\": "src/",
            "Snicco\\Blade\\": "packages/blade/src",
            "Snicco\\Database\\": "packages/eloquent/src",
            "Snicco\\EventDispatcher\\": "packages/better-wp-hooks/src",
            "Snicco\\Mail\\": "packages/better-wp-mail/src",
            "Snicco\\View\\": "packages/view/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snicco\\": "src/",
            "Tests\\": "tests/",
            "Tests\\Blade\\": "packages/blade/tests",
            "Tests\\Database\\": "packages/eloquent/tests",
            "Tests\\EventDispatcher\\": "packages/better-wp-hooks/tests",
            "Tests\\Mail\\": "packages/better-wp-mail/tests",
            "Tests\\View\\": "packages/view/tests"
        }
    },
    "replace": {
        "sniccowp/better-wp-hooks": "self.version",
        "sniccowp/better-wp-mail": "self.version",
        "sniccowp/blade": "self.version",
        "sniccowp/eloquent": "self.version",
        "sniccowp/view": "self.version"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}