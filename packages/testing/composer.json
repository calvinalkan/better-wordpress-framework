{
  "name": "sniccowp/testing",
  "description": "Provides testing capabilities for applications built with sniccowp/core",
  "minimum-stability": "stable",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.3",
    "sniccowp/core": "dev-master",
    "mockery/mockery": "^1.4.2",
    "phpunit/phpunit": "^9.5",
    "lucatume/wp-browser": "^3.0.0"
  },
  "autoload": {
    "psr-4": {
      "Snicco\\Testing\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Testing\\": "tests"
    }
  }
}