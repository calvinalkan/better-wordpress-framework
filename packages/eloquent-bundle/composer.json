{
  "name": "sniccowp/eloquent-bundle",
  "description": "Integrates sniccowp/eloquent with the sniccowp/core package.",
  "minimum-stability": "stable",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.3",
    "sniccowp/eloquent": "dev-master",
    "sniccowp/core": "dev-master"
  },
  "require-dev": {
    "fakerphp/faker": "^1.0.0-stable"
  },
  "autoload": {
    "psr-4": {
      "Snicco\\EloquentBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\EloquentBundle\\": "tests"
    }
  }
}